    },
    "expectedStatus": 400
  },
  {
    "testName": "Expire data : number",
    "body": {
      "petId": 1,
      "expiresAt": 1
    },
    "expectedStatus": 400
  },
  {
    "testName": "Expire data : boolean",
    "body": {
      "petId": 1,
      "expiresAt": true
    },
    "expectedStatus": 400
  },
  {
    "testName": "Expire data : array",
    "body": {
      "petId": 1,
      "expiresAt": ["2024-07-23T12:37:54.471Z"]
    },
    "expectedStatus": 400
  },
  {
    "testName": "Expire data : object",
    "body": {
      "petId": 1,
      "expiresAt": { "value": "2024-07-23T12:37:54.471Z" }
    },
    "expectedStatus": 400
  }
