    "expectedStatus": 400
  },
  {
    "testName": "Pet id : null",
    "body": {
      "petId": null,
      "expiresAt": "2024-07-23T12:37:54.471Z"
    },
    "expectedStatus": 400
  },
  {
    "testName": "Pet id : non existing",
    "body": {
      "petId": 111111111,
      "expiresAt": "2024-07-23T12:37:54.471Z"
    },
    "expectedStatus": 400
  },
  {
    "testName": "Pet id : string",
    "body": {
      "petId": "1",
      "expiresAt": "2024-07-23T12:37:54.471Z"
    },
    "expectedStatus": 400
  },
  {
    "testName": "Pet id : array",
    "body": {
      "petId": ["1"],
      "expiresAt": "2024-07-23T12:37:54.471Z"
    },
    "expectedStatus": 400
  },
  {
    "testName": "Pet id : object",
    "body": {
